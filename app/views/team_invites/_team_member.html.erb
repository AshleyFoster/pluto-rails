<div class="member">
  <%= image_tag  gravatar_url(team_invite.email, 40) %>
  <div class="member-email">
    <%= team_invite.email %>
  </div>
  <div class="member-status">
    <% if team_invite.email == TeamUser::MEMBER %>
      <div class="actions">
        <%= link_to "Remove User", "#" %>
      </div>
    <% else %>
      <p>Pending</p>
      <div class="actions">
        <%= link_to "Cancel Invite", "#" %>
      </div>
    <% end %>
  </div>
</div>
